{
	"compilerOptions": {
		"module": "ES6",
		"target": "ES6",
		"jsx": "react-jsx",
		"noImplicitAny": true,
		"removeComments": true,
		"preserveConstEnums": true,
		"sourceMap": true,
		"noUnusedLocals": true,
		"allowSyntheticDefaultImports": true,
		"baseUrl": "."
	},
	"include": [
		"src/**/*"
	],
	"exclude": ["node_modules"]
}
