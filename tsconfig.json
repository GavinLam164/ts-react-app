{
	"compilerOptions": {
		"module": "ES6",
		"target": "ES6",
		"jsx": "react-jsx",
		"noImplicitAny": true,
		"removeComments": true,
		"preserveConstEnums": true,
		"sourceMap": true,
		"noUnusedLocals": true,
		"allowSyntheticDefaultImports": true,
		"baseUrl": ".",
		"paths": {
			"react/jsx-runtime": ["node_modules/reawct/jsx-dev-runtime"]
		}
	},
	"include": [
		"src/**/*"
	],
	"exclude": ["node_modules"]
}
